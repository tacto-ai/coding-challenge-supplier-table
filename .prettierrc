{
  "bracketSpacing": true,
  "importOrder": ["^@mui/(.*)$", "^src/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "overrides": [
    {
      "files": "*.json",
      "options": { "printWidth": 160, "singleQuote": true, "tabWidth": 2, "useTabs": false, "proseWrap": "always", "trailingComma": "es5" }
    },
    { "files": "*.md", "options": { "printWidth": 80, "singleQuote": true, "tabWidth": 2, "useTabs": false, "proseWrap": "always" } }
  ],
  "printWidth": 160,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
